\begin_inset Formula 
\[
r^{2}-2r+1=0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
r=1,r=1
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
x=e^{t}\left(C_{1}+C_{2}t\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
x'=e^{t}\left(C_{1}+C_{2}t\right)+e^{t}C_{2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
x'=e^{t}\left(C_{1}+C_{2}+C_{2}t\right)
\]

\end_inset


\end_layout

