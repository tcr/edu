\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
y=a_{0}+a_{1}x+a_{2}x^{2}+a_{3}x^{3}+\ldots=\sum_{n=0}^{\infty}a_{n}x^{n}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
y'=\sum_{n=1}^{\infty}na_{n}x^{n-1}
\]

\end_inset


\begin_inset Formula 
\[
