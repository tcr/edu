\begin_inset Formula 
\[
f(t)=1,\quad t\geq0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mathcal{L}\left\{ 1\right\} =\int_{0}^{\infty}e^{-st}dt=-\lim_{A\rightarrow\infty}\left.\frac{e^{-st}}{s}\right|_{0}^{A}=\frac{1}{s},\quad s>0
\]

\end_inset


